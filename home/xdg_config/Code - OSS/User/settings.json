{
  // General Settings
  "editor.autoIndent": "full",
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.detectIndentation": true,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.lineNumbers": "on",
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.linkedEditing": true,
  "editor.rulers": [80],
  "editor.tabCompletion": "on",
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 80,
  "editor.fontFamily": "Cousine,Liberation Mono,DejaVu Sans Mono,Courier New,monospace",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.associations": {
    ".php_cs": "php",
    ".php_cs.dist": "php",
    ".versionrc": "json",
    "*.xml.dist": "xml"
  },
  "security.workspace.trust.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.drawBoldTextInBrightColors": true,
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "/bin/bash"
    },
    "zsh": {
      "path": "/bin/zsh"
    }
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    }
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "window.newWindowDimensions": "maximized",
  "workbench.colorTheme": "Coldark - Cold",
  "workbench.editor.closeEmptyGroups": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.preferredDarkColorTheme": "Coldark - Dark",
  "workbench.preferredLightColorTheme": "Coldark - Cold",
  // Git Settings
  "git.enableCommitSigning": true,
  // Languages Settings
  // HTML
  "html.autoClosingTags": true,
  "html.format.contentUnformatted": "pre,code,textarea",
  "html.format.enable": true,
  "html.format.endWithNewline": false,
  "html.format.extraLiners": null,
  "html.format.indentInnerHtml": true,
  "html.format.wrapAttributes": "auto",
  "html.suggest.html5": true,
  "html.validate.scripts": true,
  "html.validate.styles": true,
  "[html]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // CSS
  "css.validate": true,
  "[css]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // LESS
  "less.validate": true,
  "[less]": {
    "editor.tabSize": 2
  },
  // SCSS
  "scss.validate": true,
  "[scss]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // Javascript
  "javascript.autoClosingTags": true,
  "javascript.format.enable": true,
  "javascript.format.semicolons": "insert",
  "javascript.preferences.quoteStyle": "double",
  "javascript.suggest.autoImports": true,
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.suggest.completeJSDocs": true,
  "javascript.suggest.enabled": true,
  "javascript.validate.enable": true,
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.tabSize": 2
  },
  // JSON
  "json.format.enable": true,
  "[json]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // Markdown
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": false,
    "files.trimTrailingWhitespace": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[mdx]": {
    "editor.quickSuggestions": false,
    "editor.wordWrap": "on",
    "files.trimTrailingWhitespace": false
  },
  // PHP
  "php.suggest.basic": true,
  "php.validate.enable": true,
  "php.validate.executablePath": "/usr/bin/php",
  "php.validate.run": "onSave",
  "[php]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "junstyle.php-cs-fixer"
  },
  // TypeScript
  "typescript.autoClosingTags": true,
  "typescript.format.enable": true,
  "typescript.suggest.enabled": true,
  "typescript.validate.enable": true,
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // YAML
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced"
  },
  // Shell Script
  "[shellscript]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "files.eol": "\n"
  },
  "[vue]": {
    "editor.defaultFormatter": "johnsoncodehk.volar"
  },
  // Extensions Settings
  // Code Runner
  "code-runner.enableAppInsights": false,
  // cSpell
  "cSpell.enableFiletypes": ["twig"],
  "cSpell.userWords": [
    "ABSPATH",
    "apcom",
    "aria",
    "armandphilippot",
    "bloginfo",
    "browserslist",
    "coldark",
    "commitlint",
    "datetime",
    "dircolors",
    "gedit",
    "gifsicle",
    "Gitlab",
    "htdocs",
    "itemprop",
    "itemscope",
    "itemtype",
    "keepachangelog",
    "kses",
    "labelledby",
    "lingui",
    "Manjaro",
    "minmax",
    "mixins",
    "mkcert",
    "mozjpeg",
    "muplugin",
    "optipng",
    "pacman",
    "philippot",
    "phpcs",
    "phpmyadmin",
    "Pico",
    "rtlcss",
    "setfacl",
    "stylelint",
    "submodule",
    "svgr",
    "Symfony",
    "traefik",
    "vsce",
    "wcag",
    "wp"
  ],
  // Emmet
  "emmet.includeLanguages": {
    "twig": "html",
    "smarty": "html"
  },
  // Path Intellisense
  "path-intellisense.autoSlashAfterDirectory": true,
  // PHP Intelephense
  "intelephense.stubs": [
    "apache",
    "bcmath",
    "bz2",
    "calendar",
    "com_dotnet",
    "Core",
    "ctype",
    "curl",
    "date",
    "dba",
    "dom",
    "enchant",
    "exif",
    "FFI",
    "fileinfo",
    "filter",
    "fpm",
    "ftp",
    "gd",
    "gettext",
    "gmp",
    "hash",
    "iconv",
    "imap",
    "intl",
    "json",
    "ldap",
    "libxml",
    "mbstring",
    "meta",
    "mysqli",
    "oci8",
    "odbc",
    "openssl",
    "pcntl",
    "pcre",
    "PDO",
    "pdo_ibm",
    "pdo_mysql",
    "pdo_pgsql",
    "pdo_sqlite",
    "pgsql",
    "Phar",
    "posix",
    "pspell",
    "readline",
    "Reflection",
    "session",
    "shmop",
    "SimpleXML",
    "snmp",
    "soap",
    "sockets",
    "sodium",
    "SPL",
    "sqlite3",
    "standard",
    "superglobals",
    "sysvmsg",
    "sysvsem",
    "sysvshm",
    "tidy",
    "tokenizer",
    "xml",
    "xmlreader",
    "xmlrpc",
    "xmlwriter",
    "xsl",
    "Zend OPcache",
    "zip",
    "zlib",
    "wordpress"
  ],
  "intelephense.telemetry.enabled": false,
  // PHP Sniffer
  "phpSniffer.autoDetect": true,
  // PHP CS Fixer
  "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
  "php-cs-fixer.formatHtml": true,
  "php-cs-fixer.onsave": true,
  "php-cs-fixer.lastDownload": 1645700557465,
  // Prettier
  "prettier.resolveGlobalModules": true,
  "prettier.requireConfig": true,
  // YAML
  "redhat.telemetry.enabled": false,
  // Twig Language 2
  "twig-language-2.indentStyle": "space",
  "twig-language-2.spaceClose": true,
  "twig-language-2.tabSize": 4,
  "sonarlint.ls.javaHome": "/home/armand/.vscode-oss/extensions/sonarsource.sonarlint_managed-jre/jre/jdk-11.0.13+8-jre",
  "sonarlint.rules": {
    "php:S101": {
      "level": "off"
    }
  }
}
